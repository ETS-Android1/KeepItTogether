<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.keep_it_together">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".YourHouse"></activity>
        <activity android:name=".Graphs" />
        <activity android:name=".CompletedTasks" />
        <activity android:name=".YourTasks" />
        <activity android:name=".Edit" />
        <activity android:name=".ToDo" />
        <activity android:name=".Add" />
        <activity android:name=".View" />
        <activity android:name=".MainMenu" />
        <activity android:name=".Register" />
        <activity android:name=".AccountLogin">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>